 <!-- Contact -->
 <div id="contact" class="w3-center w3-padding-hor-64">
  <span class="w3-xlarge w3-bottombar w3-border-green w3-padding-hor-16">{{'contact'|translate}}</span>
</div>
<form class="w3-container w3-margin-bottom">
  <div class="w3-group">
    <label>{{'name'|translate}}</label>
    <input class="w3-input w3-border w3-hover-border-black" style="width:100%;" type="text">
  </div>
  <div class="w3-group">
    <label>{{'subject'|translate}}</label>
    <input class="w3-input w3-border w3-hover-border-black" style="width:100%;" type="text">
  </div>
  <div class="w3-group">
    <label>{{'text'|translate}}</label>
    <textarea class="w3-input w3-border w3-hover-border-black" style="width:100%;" rows= 5></textarea>
  </div>
  <button type="button" class="w3-btn w3-btn-block w3-padding-hor-12 last">Send</button>
</form>
</div>